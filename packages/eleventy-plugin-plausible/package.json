{
  "name": "@jackdbd/eleventy-plugin-plausible",
  "version": "1.0.1-canary.1",
  "description": "Eleventy plugin that retrieves analytics from Plausible when building your site.",
  "author": {
    "name": "Giacomo Debidda",
    "email": "giacomo@giacomodebidda.com",
    "url": "https://giacomodebidda.com/"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "11ty",
    "plugin",
    "analytics",
    "plausible"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jackdbd/undici"
  },
  "homepage": "https://github.com/jackdbd/undici/tree/main/packages/eleventy-plugin-plausible#readme",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./schemas": "./lib/schemas.js",
    "./package.json": "./package.json"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "build": "run-s 'build:code' 'build:docs' --print-label",
    "build:code": "rm -rf lib/ && mkdir lib && cp -r src/* lib",
    "build:docs": "run-s 'docs:readme' --print-label",
    "clean": "rimraf lib/",
    "dev": "run-p 'build:watch' 'test:watch' --print-label",
    "docs:readme": "tsm ../../scripts/readme.ts eleventy-plugin-plausible",
    "format": "../../scripts/format.mjs",
    "lint": "eslint --config ../../config/eslint-no-typescript.cjs",
    "nuke": "npm run clean && rimraf node_modules 'package-lock.json'",
    "precommit": "npx lint-staged --config ../../config/lint-staged.cjs",
    "size": "pkg-size ./lib --sort-by=brotli --ignore-files {*.d.ts,*.map}",
    "test": "node --test --experimental-test-coverage",
    "test:ci": "node --test --experimental-test-coverage",
    "test:watch": "node --test --watch"
  },
  "dependencies": {
    "@jackdbd/plausible-client": "^1.0.2",
    "joi": "^17.12.0"
  },
  "peerDependencies": {
    "@11ty/eleventy": ">=2.0.0 || 3.0.0-alpha.4",
    "debug": ">=4.0.0"
  },
  "devDependencies": {
    "@jackdbd/eleventy-test-utils": "*"
  }
}
