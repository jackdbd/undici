{
  "name": "@jackdbd/demo-site",
  "version": "0.0.1",
  "description": "11ty site test and showcase the usage of several 11ty plugins",
  "author": {
    "name": "Giacomo Debidda",
    "email": "giacomo@giacomodebidda.com",
    "url": "https://giacomodebidda.com/"
  },
  "private": true,
  "keywords": [
    "11ty"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jackdbd/undici"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "eleventy --config config/eleventy.cjs",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf _site/ .cache/",
    "lint": "eslint --config ../../config/eslint.cjs",
    "nuke": "npm run clean && rimraf node_modules 'package-lock.json'",
    "precommit": "npx lint-staged --config ../../config/lint-staged.cjs",
    "serve": "http-server ./_site/ --port 8090",
    "size": "pkg-size ./_site --sort-by=brotli --ignore-files {*.d.ts,*.map}"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0-alpha.4",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@jackdbd/eleventy-plugin-ensure-env-vars": "^1.0.0",
    "@jackdbd/eleventy-plugin-plausible": "^1.0.0",
    "@jackdbd/eleventy-plugin-telegram": "^1.1.1",
    "@jackdbd/eleventy-plugin-text-to-speech": "^2.0.1",
    "eleventy-plugin-helmet": "^0.2.2"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
